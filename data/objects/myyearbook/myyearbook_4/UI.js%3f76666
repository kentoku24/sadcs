UI=function(){function init(){$(document).trigger('UI.loaded').unbind('UI.loaded')}init()};UI.initialize=function($element,uiType,data){if(typeof UI[uiType]!=='function'){throw('Unknown UI Element type: '+uiType)}new UI[uiType]($element,data)};UI.Button=function($element,data){};UI.GiveGiftButton=function($element,data){if(data.recipientId!==0){$element.mybActionIcons({userid:data.recipientId},{token:stok,doGift:true,bindOnly:true})}if(typeof data.gender==='string'){$element.text('Grab '+(data.gender==='male'?'his':'her')+' attention with a gift')}};UI.SpotlightBar=function($element,data){new SpotlightBar($element,data.key,data.members)};UI.SocialTheaterButton=function($element,data){if(data.destination=='ST Mobile'){$element.addClass('SocialTheaterButton-medium')}else{$element.addClass('SocialTheaterButton-xlarge')}};var uiManager=new UI();