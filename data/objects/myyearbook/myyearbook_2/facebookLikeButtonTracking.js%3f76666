$(document).ready(function(){if(!document.getElementById('fb-root')){var div=document.createElement('div');div.setAttribute('id','fb-root');window.document.body.insertBefore(div,window.document.body.firstChild)}var e=document.createElement('script');e.src=document.location.protocol+'//connect.facebook.net/en_US/all.js';e.async=true;document.getElementById('fb-root').appendChild(e);if(typeof(MyYearbook)!='undefined'&&typeof(MyYearbook.URLs)!='undefined'){var channelURL=MyYearbook.URLs[MyYearbook.currentServiceName]+'xd_receiver.htm';var likeURL=MyYearbook.URLs['www']+'apps/record/facebookLike/like/';var unlikeURL=MyYearbook.URLs['www']+'apps/record/facebookLike/unlike/'}else{var channelURL=null;var likeURL=SITE_URL+'apps/record/facebookLike/like/';var unlikeURL=SITE_URL+'apps/record/facebookLike/unlike/'}window.fbAsyncInit=function(){if(typeof Connect==='undefined'||typeof Connect.networks[Connect.Facebook]==='undefined'||Connect.networks[Connect.Facebook].getConfig('assetsLoaded')!==true){FB.init({appId:facebookLikeButtonAppId,xfbml:true,channelUrl:channelURL,status:true,cookie:true,logging:false,oauth:true})}var likes=new Array();FB.Event.subscribe("edge.create",function(response,widget){if(likes[response]){return false}likes[response]=1;var ref=widget.getAttribute('ref');var refs=ref.split('_');var category=refs.pop();if(category==''){category='Unknown'}switch(category){case'HBL':$('#footer').height($('#footer').height()+120);break;case'StatusUpdate':case'PhotoUpload':case'AskMe':case'RateMe':case'YouTubeVideo':case'TruthGame':case'ChatterPoll':case'LivePhoto':if(!$.browser.msie){$('#maincontainer').css({'overflow':'visible'});$('div.HALObject_noWrapper','#HAL_wide').css({'overflow':'visible'});$('div.feedItemRightContainer','#feed').css({'overflow':'visible'});break}}$.ajax({type:'get',dataType:'jsonp',cache:false,url:likeURL+category,success:function(){},error:function(){},complete:function(){}})});var unlikes=new Array();FB.Event.subscribe("edge.remove",function(response,widget){if(unlikes[response]){return false}unlikes[response]=1;var ref=widget.getAttribute('ref');var refs=ref.split('_');var category=refs.pop();if(category==''){category='Unknown'}$.ajax({type:'get',dataType:'jsonp',cache:false,url:unlikeURL+category,success:function(){},error:function(){},complete:function(){}})})}});FacebookLikeButton={create:function(likeURL,refValue,action,parentId){var elem=$(document.createElement('fb:like'));elem.attr('href',likeURL);elem.attr('ref',refValue);elem.attr('action',action);elem.attr('layout','button_count');elem.attr('width',75);$('#'+parentId).empty().append(elem);Â if(typeof FB!=='undefined'){FB.XFBML.parse($('#'+parentId).get(0))}}};